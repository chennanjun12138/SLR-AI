<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cnj.slrbackend.mapper.SignWordMapper">

    <!-- 字段映射 -->
    <resultMap id="BaseResultMap" type="com.cnj.slrbackend.model.entity.SignWord">
        <id     property="id"             column="id"/>
        <result property="word"           column="word"/>
        <result property="videoUrl"       column="videoUrl"/>
        <result property="actionDesc"     column="actionDesc"/>
        <result property="chineseMeaning" column="chineseMeaning"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,word,videoUrl,actionDesc,chineseMeaning
    </sql>
</mapper>
